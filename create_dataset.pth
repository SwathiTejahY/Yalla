      "source": [
        "import torch\n",
        "import numpy as np\n",
        "\n",
        "# Generate dummy dataset\n",
        "X_dummy = np.random.rand(100, 10).astype(np.float32)  # 100 samples, 10 features each\n",
        "y_dummy = np.random.randint(0, 5, size=(100,)).astype(np.int64)  # 100 labels (0-4 classes)\n",
        "\n",
        "# Correct dataset structure\n",
        "corrected_data = {\"X\": X_dummy, \"y\": y_dummy}\n",
        "\n",
        "# Save the corrected file\n",
        "torch.save(corrected_data, \"corrected_dataset.pth\")\n",
        "print(\"Corrected dataset saved as 'corrected_dataset.pth'\")\n"
      ]
    }
  ]
